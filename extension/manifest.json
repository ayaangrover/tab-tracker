{
  "manifest_version": 3,
  "name": "Tab Tracker",
  "version": "2.0",
  "description": "A tracker for teachers, made by Ayaan Grover",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "<all_urls>",
    "http://localhost:3000/"
  ],
  "externally_connectable": {
    "matches": ["*://localhost/*"]
  }
}